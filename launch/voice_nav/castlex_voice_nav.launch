<?xml version='1.0' encoding='utf-8'?>

<launch>
  <include file="$(find castlex_navigation)/launch/nav_ws/castlex_nav.launch"/>
    <!--打开语音唤醒-->
    <include file="$(find castlex_voice_system)/launch/castlex_awake.launch"/>
    <!--打开命令词识别-->
    <node pkg = "castlex_voice_system" type = "voice_nav_order_node" name = "voice_nav_order_node" output="screen"/>
    <!--打开命令词解析-->
    <node pkg = "castlex_voice_system" type = "voice_nav.py" name = "XML_Analysis" output="screen">
        <param name="failed_file_path" value="$(find castlex_voice_system)/params/voice/failed.mp3" />
        <param name="Received_file_path" value="$(find castlex_voice_system)/params/voice/Received.mp3" />
        <param name="ReEnterAuido_file_path" value="$(find castlex_voice_system)/params/voice/ReEnterAuido.mp3" />
        <param name="file_path_a" value="$(find castlex_navigation)/voice/ai/A1.mp3" />
        <param name="file_path_b" value="$(find castlex_navigation)/voice/ai/B1.mp3" />
        <param name="file_path_c" value="$(find castlex_navigation)/voice/ai/C1.mp3" />
        <param name="file_path_d" value="$(find castlex_navigation)/voice/ai/D1.mp3" />
    </node>

  <node pkg = "castlex_navigation" type = "voice_goal_nav.py" name = "voice_goal_nav" output="screen" />
  
</launch>
